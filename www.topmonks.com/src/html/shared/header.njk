  <script>
    var appBar = document.querySelector(".tm-app-bar");
    var hamburger = document.querySelector(".tm-hamburger__container");
    var menu = document.querySelector(".tm-menu");
    hamburger.addEventListener("click", toggleMenu(hamburger));
    function toggleMenu(hamburger) {
      return function() {
        var isOpen = hamburger.classList.toggle("tm-hamburger__container--open");
        hamburger.setAttribute("aria-expanded", isOpen);
        menu.setAttribute("aria-hidden", !isOpen);
        appBar.classList.toggle("tm-app-bar--open");
      }
    }

    var lastPosition = 0;
    var ticking = false;
    document.addEventListener("scroll", function() {
      lastPosition = window.scrollY;
      if (!ticking) {
        window.requestAnimationFrame(function() {
          if (lastPosition > 0) {
            appBar.classList.add("tm-app-bar--sticky");
          }
          else {
            appBar.classList.remove("tm-app-bar--sticky");
          }
          ticking = false;
        });
        ticking = true;
      }
    });

    window.addEventListener("resize", function() {
      if (!ticking) {
        window.requestAnimationFrame(function() {
          if (window.matchMedia("(min-width: 841px)").matches) {
            menu.classList.add("tm-menu--horizontal");
            menu.setAttribute("aria-hidden", "false");
          }
          else {
            menu.classList.remove("tm-menu--horizontal");
            menu.setAttribute("aria-hidden", "true");
          }
          ticking = false;
        });
        ticking = true;
      }
    });

    if (window.matchMedia("(min-width: 841px)").matches) {
      var isHorizontal = menu.classList.toggle("tm-menu--horizontal");
    }
    else {
      menu.setAttribute("aria-hidden", true);
    }
  </script>
