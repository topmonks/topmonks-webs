    "pulumi:refresh": "AWS_PROFILE=topmonks pulumi refresh",
    "pulumi:login": "AWS_PROFILE=topmonks pulumi login --cloud-url s3://pulumi.tmcloud.io && AWS_PROFILE=topmonks pulumi stack select topmonks-webs",
