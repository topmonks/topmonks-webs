  <style>
    :root {
      --tm-black-title: #fff;
    }

    body {
      background-color: #000;
      color: #fff;
    }

    main {
      display: grid;
      grid-template-columns: 2fr minmax(320px, 400px);
      grid-template-rows: 100px 5fr;
      gap: 0 0;
      grid-template-areas:
        ". ."
        ". .";
    }

    main header {
      order: 0;
      text-align: left;
      margin: 0;
      padding: 16px 16px 0;
      display: flex;
    }

    main header svg {
      vertical-align: bottom;
      margin-right: 24px;
    }

    main header h1 {
      text-align: left;
      font-size: 28px;
      line-height: 26px;
      display: flex;
      flex-direction: column;
    }

    main header h1 small {
      font-size: 13px;
      line-height: 15px;
      font-weight: normal;
      text-transform: lowercase;
    }

    @media screen and (min-width: 480px) {
      main header {
        padding: 16px 24px 0;
      }
    }

    main .slido-box {
      order: 1;
      row-span: 2;
      height: 100vh;
    }

    main .video-box {
      order: 2;
    }

    .copy-and-media {
      background-color: #fff;
      color: #0c0449;
    }

    @media screen and (max-width: 480px) {
      body {
        background-size: auto;
      }
    }

    .video-box {
      position: relative;
      padding-bottom: 56.25%; /* 16:9 */
      height: 0;
    }

    .video-box iframe {
      width: 100%;
      height: 100%;
      position: absolute;
      top: 0;
      left: 0;
    }

    footer {
      color: #fff;
      background: #2c2c2c;
    }

    footer ul {
      padding: 0;
      list-style: none;
    }

    footer ul li a {
      color: rgb(240, 107, 79);
      text-decoration: none;
    }

     .email a {
      color: rgb(240, 107, 79);
    }

    footer ul li a:hover {
      text-decoration: underline;
    }

    .footer_social-buttons {
      text-align: right;
      align-self: center;
    }
    .footer_social-buttons a {
      text-decoration: none;
    }
  </style>
</head>

